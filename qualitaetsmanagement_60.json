{
  "topicId": "qualitaetsmanagement",
  "title": "Qualitätsmanagement im Rettungsdienst",
  "questions": [
    {
      "id": "frage_001",
      "type": "single",
      "stem": "Wie definiert sich Qualität im QM?",
      "options": [
        {
          "id": "a",
          "text": "Grad, in dem ein Produkt/Dienstleistung Anforderungen erfüllt",
          "isCorrect": true,
          "explanation": "Qualität beschreibt die Erfüllung von Anforderungen."
        },
        {
          "id": "b",
          "text": "Maximaler Gewinn",
          "isCorrect": false
        },
        {
          "id": "c",
          "text": "Nur Kundenzufriedenheit",
          "isCorrect": false
        }
      ]
    },
    {
      "id": "frage_002",
      "type": "single",
      "stem": "Welche drei Dimensionen von Qualität unterscheidet man?",
      "options": [
        {
          "id": "a",
          "text": "Struktur-, Prozess- und Ergebnisqualität",
          "isCorrect": true,
          "explanation": "Diese Dimensionen sind Grundlage im Gesundheitswesen."
        },
        {
          "id": "b",
          "text": "Input, Output, Feedback",
          "isCorrect": false
        },
        {
          "id": "c",
          "text": "Planung, Durchführung, Kontrolle",
          "isCorrect": false
        }
      ]
    },
    {
      "id": "frage_003",
      "type": "single",
      "stem": "Welches Ziel verfolgt ein Leitbild im QM?",
      "options": [
        {
          "id": "a",
          "text": "Orientierung und Identifikation für Mitarbeiter",
          "isCorrect": true,
          "explanation": "Leitbilder geben Werte und Ziele des Unternehmens vor."
        },
        {
          "id": "b",
          "text": "Nur Gewinnmaximierung",
          "isCorrect": false
        },
        {
          "id": "c",
          "text": "Reduktion von Dokumentation",
          "isCorrect": false
        }
      ]
    },
    {
      "id": "frage_004",
      "type": "single",
      "stem": "Welches Ziel hat ein QM-System nach ISO 9001?",
      "options": [
        {
          "id": "a",
          "text": "Kontinuierliche Verbesserung und Kundenzufriedenheit",
          "isCorrect": true,
          "explanation": "Die ISO 9001 fordert die ständige Verbesserung."
        },
        {
          "id": "b",
          "text": "Einmalige Zertifizierung",
          "isCorrect": false
        },
        {
          "id": "c",
          "text": "Nur interne Kontrolle",
          "isCorrect": false
        }
      ]
    },
    {
      "id": "frage_005",
      "type": "single",
      "stem": "Welche Dimensionen werden im KTQ®-Verfahren beurteilt?",
      "options": [
        {
          "id": "a",
          "text": "Patientenorientierung, Mitarbeiterorientierung, Sicherheit, Führung, QM, Ergebnisse",
          "isCorrect": true,
          "explanation": "KTQ® umfasst sechs Dimensionen."
        },
        {
          "id": "b",
          "text": "Nur Patientenorientierung",
          "isCorrect": false
        },
        {
          "id": "c",
          "text": "Nur Finanzen",
          "isCorrect": false
        }
      ]
    },
    {
      "id": "frage_006",
      "type": "single",
      "stem": "Welche Dokumente gehören ins QM-Handbuch?",
      "options": [
        {
          "id": "a",
          "text": "Verfahrensanweisungen, Arbeitsanweisungen, Organigramm",
          "isCorrect": true,
          "explanation": "Sie strukturieren die Prozesse im QM."
        },
        {
          "id": "b",
          "text": "Nur Einsatzberichte",
          "isCorrect": false
        },
        {
          "id": "c",
          "text": "Nur Dienstpläne",
          "isCorrect": false
